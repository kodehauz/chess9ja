<div class="right_col" role="main">
    <div class="row">
      <div class="col-md-8 col-sm-12 col-xs-12">
        {{ page.content }}
        {{ page.dasboard_content }}
        <div class="row">
          <div class="col-sm-6 col-xs-12">
            {{ page.dashboard_bottom_left }}
          </div>
          <div class="col-sm-6 col-xs-12">
            {{ page.dashboard_bottom_right }}
          </div>
        </div>

      </div>


      <div class="col-md-4">
        <div class="x_panel">
          <div class="x_title">
            <h2>My Current Games</h2>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <article class="media event">
              <a class="pull-left date">
                <p class="month">April</p>
                <p class="day">23</p>
              </a>
              <div class="media-body">
                <a class="title" href="#">Item One Title</a>
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
              </div>
            </article>
            <article class="media event">
              <a class="pull-left date">
                <p class="month">April</p>
                <p class="day">23</p>
              </a>
              <div class="media-body">
                <a class="title" href="#">Item Two Title</a>
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
              </div>
            </article>



          </div>
        </div>
        <!-- My Current Game Region ends here-->


        <div class="x_panel">
          <div class="x_title">
            <h2>Performance Chart</h2>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <h4>Statistic off all games played</h4>
            <div class="widget_summary">
              <div class="w_left w_25">
                <span>Wins</span>
              </div>
              <div class="w_center w_55">
                <div class="progress">
                  <div class="progress-bar bg-green" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="200" style="width: 20%;">
                    <span class="sr-only">60% Complete</span>
                  </div>
                </div>
              </div>
              <div class="w_right w_20">
                <span>6</span>
              </div>
              <div class="clearfix"></div>
            </div>

            <div class="widget_summary">
              <div class="w_left w_25">
                <span>Losses</span>
              </div>
              <div class="w_center w_55">
                <div class="progress">
                  <div class="progress-bar bg-green" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="200" style="width: 40%;">
                    <span class="sr-only">60% Complete</span>
                  </div>
                </div>
              </div>
              <div class="w_right w_20">
                <span>50</span>
              </div>
              <div class="clearfix"></div>
            </div>

          </div>
        </div>




        <!-- My Favorite opponent Region Begins here-->
        <div class="x_panel">
          <div class="x_title">
            <h2>Favorite Opponent</h2>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <article class="media event">
              <a class="pull-left date">
                <p class="month">April</p>
                <p class="day">23</p>
              </a>
              <div class="media-body">
                <a class="title" href="#">Item One Title</a>
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
              </div>
            </article>

            <article class="media event">
              <a class="pull-left date">
                <p class="month">April</p>
                <p class="day">23</p>
              </a>
              <div class="media-body">
                <a class="title" href="#">Item Two Title</a>
                <p>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</p>
              </div>
            </article>


          </div>
        </div>
        <!-- My Current Game Region ends here-->

        <div class="x_panel tile fixed_height_320">
          <div class="x_title">
            <h2>Top Players</h2>

            <div class="clearfix"></div>
          </div>
          <div class="x_content">
            <ul class="list-group">
              <li class="list-group-item">Item 1</li>
              <li class="list-group-item">Item 2</li>
              <li class="list-group-item">Item 3</li>
              <li class="list-group-item">Item 3</li>
              <li class="list-group-item">Item 3</li>
            </ul>
          </div>
        </div>
      </div>


    </div>

</div>
